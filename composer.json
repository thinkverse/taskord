{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "cyrildewit/eloquent-viewable": "dev-laravel-8-support",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "graham-campbell/markdown": "^13.0",
        "graham-campbell/throttle": "^8.1",
        "grosv/laravel-passwordless-login": "^1.2",
        "guzzlehttp/guzzle": "^7.0",
        "laravel-notification-channels/telegram": "^0.5.0",
        "laravel/framework": "^8.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "livewire/livewire": "^2.0",
        "multicaret/laravel-acquaintances": "^3.3",
        "nesbot/carbon": "^2.36",
        "qcod/laravel-gamify": "^1.0",
        "rebing/graphql-laravel": "^5.1",
        "rennokki/laravel-eloquent-query-cache": "^2.4",
        "sentry/sentry-laravel": "1.9.0",
        "spatie/emoji": "^2.1",
        "spatie/laravel-backup": "^6.11",
        "stevebauman/purify": "^3.0",
        "valorin/pwned-validator": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.2",
        "wnx/laravel-stats": "^2.4"
    },
    "repositories": [
        {
            "url": "https://github.com/reliq/eloquent-viewable",
            "type": "vcs"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
