{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-heroicons": "1.1.0",
        "cyrildewit/eloquent-viewable": "6.0.2",
        "doctrine/dbal": "3.1.0",
        "enlightn/enlightn": "1.22.0",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.4",
        "graham-campbell/throttle": "8.1.0",
        "grosv/laravel-passwordless-login": "1.6.0",
        "guzzlehttp/guzzle": "7.3.0",
        "intervention/image": "2.5.1",
        "irazasyed/telegram-bot-sdk": "3.4.1",
        "jantinnerezo/livewire-alert": "2.1.5",
        "laravel/envoy": "2.7.0",
        "laravel/framework": "8.40.0",
        "laravel/horizon": "5.7.6",
        "laravel/socialite": "5.2.3",
        "laravel/tinker": "2.6.1",
        "laravel/ui": "3.2.1",
        "league/commonmark": "1.6.0",
        "livewire/livewire": "2.4.4",
        "mll-lab/laravel-graphql-playground": "2.5.0",
        "multicaret/laravel-acquaintances": "3.5.1",
        "nesbot/carbon": "2.46.0",
        "nicolaslopezj/searchable": "1.13.0",
        "nuwave/lighthouse": "5.7.2",
        "propaganistas/laravel-disposable-email": "2.0.19",
        "qcod/laravel-gamify": "1.0.5",
        "renatomarinho/laravel-page-speed": "2.0.0",
        "rennokki/laravel-eloquent-query-cache": "2.6.0",
        "sentry/sentry-laravel": "2.5.1",
        "silviolleite/laravelpwa": "2.0.3",
        "spatie/laravel-activitylog": "3.17.0",
        "spatie/laravel-backup": "7.5.2",
        "stevebauman/purify": "4.0.0",
        "symfony/psr-http-message-bridge": "2.1.0"
    },
    "require-dev": {
        "facade/ignition": "2.8.4",
        "fakerphp/faker": "1.14.1",
        "mockery/mockery": "1.4.3",
        "nunomaduro/collision": "5.4.0",
        "pestphp/pest": "1.0.5",
        "pestphp/pest-plugin-laravel": "1.0.0",
        "pestphp/pest-plugin-livewire": "1.0.0",
        "phpunit/phpunit": "9.5.4",
        "wnx/laravel-stats": "2.5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi"
        ]
    }
}
